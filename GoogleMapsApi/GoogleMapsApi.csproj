<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>netstandard1.5;net45</TargetFrameworks>
    <Description>Google Maps Web Services API wrapper for .NET</Description>
    <Company>http://code.google.com/p/google-maps/</Company>
    <Product>google-maps</Product>
    <Copyright>Copyright ©  2017</Copyright>
    <AssemblyVersion>1.1.0.0</AssemblyVersion>
    <FileVersion>1.41.0.0</FileVersion>
    <PackageProjectUrl>https://github.com/maximn/google-maps</PackageProjectUrl>
    <PackageLicenseUrl>https://github.com/maximn/google-maps/blob/master/LICENSE.md</PackageLicenseUrl>
    <RepositoryUrl>https://github.com/maximn/google-maps</RepositoryUrl>
    <RepositoryType>Github</RepositoryType>
    <PackageTags>Google Maps netcore net45 netstandard15 API web services webservices .net core</PackageTags>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="System.Net.Http" Version="4.3.2" />
    <PackageReference Include="System.Runtime.Serialization.Json" Version="4.3.0" />
    <PackageReference Include="System.Runtime.Serialization.Primitives" Version="4.3.0" />
  </ItemGroup>

  <ItemGroup Condition="'$(TargetFramework)' == 'netstandard1.5'">
    <PackageReference Include="System.Net.Http.WinHttpHandler">
      <Version>4.3.0</Version>
    </PackageReference>
    <PackageReference Include="System.Security.Cryptography.Algorithms">
      <Version>4.3.0</Version>
    </PackageReference>
  </ItemGroup>

</Project>